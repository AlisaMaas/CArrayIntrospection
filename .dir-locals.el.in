((c++-mode
  (c-file-style . "Linux")
  (flycheck-c/c++-gcc-executable . @CXX@)
  (flycheck-disabled-checkers c/c++-clang)
  (flycheck-gcc-definitions . @CPPDEFINES@)
  (flycheck-gcc-include-path . @CPPPATH@)
  (flycheck-gcc-language-standard . "c++11"))
 ("SRA"
  . (c++-mode
     (c-file-style . "gnu"))))

